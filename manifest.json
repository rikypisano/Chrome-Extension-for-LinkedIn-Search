{
  "manifest_version": 2,
  "name": "LinkedIn Search Assistant",
  "description": "Search LinkedIn Like A Pro",
  "version": "1.0",
  "icons": { "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["jquery-3.3.1.min.js","content.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}